@model StudentViewModel

@{
    ViewData["Title"] = "Student Details";
}

<h2>Student Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Student Name</th>
            <td>@Model.StudentName</td>
        </tr>

        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Position</th>
            <td>@Model.Position</td>
        </tr>
        <tr>
            <th>Experience</th>
            <td>@Model.Experience</td>
        </tr>
    </tbody>
</table>
